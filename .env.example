# Google Tag Manager Provider Configuration
# Copy this file to .env and fill in your values

# Required configuration
GTM_CREDENTIAL_FILE=/path/to/your/service-account.json
GTM_ACCOUNT_ID=your-gtm-account-id
GTM_CONTAINER_ID=your-gtm-container-id
GTM_WORKSPACE_NAME=your-workspace-name

# Optional configuration
GTM_RETRY_LIMIT=15  # Default is 10, increase for more retries on rate limiting
